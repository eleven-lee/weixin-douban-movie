@font-face {
  font-family: 'icomoon';
  src:  url('fonts/icomoon.eot?iwq1cc');
  src:  url('fonts/icomoon.eot?iwq1cc#iefix') format('embedded-opentype'),
    url('fonts/icomoon.ttf?iwq1cc') format('truetype'),
    url('fonts/icomoon.woff?iwq1cc') format('woff'),
    url('fonts/icomoon.svg?iwq1cc#icomoon') format('svg');
  font-weight: normal;
  font-style: normal;
}

[class^="icon-"], [class*=" icon-"] {
  /* use !important to prevent issues with browser extensions that change fonts */
  font-family: 'icomoon' !important;
  speak: none;
  font-style: normal;
  font-weight: normal;
  font-variant: normal;
  text-transform: none;
  line-height: 1;

  /* Better Font Rendering =========== */
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

.icon-user:before {
  content: "\e900";
}
.icon-film:before {
  content: "\e901";
}
.icon-search:before {
  content: "\e902";
}
.icon-keyboard_backspace:before {
  content: "\e903";
}
.icon-chevron-thin-right:before {
  content: "\e904";
}
